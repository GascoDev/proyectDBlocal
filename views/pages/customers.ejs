<!DOCTYPE html>
<html lang="en">
    <%-include("../partials/head")%>
<body>
    <%-include("../partials/navbar")%>

    
        <!--Se ejecuta bucle forEach para recorrer los objetos que me van llegando de la DB e imprimirlos en etiquetas-->
        <% custom.forEach(data => { %>
            

            <table class="table">
                <thead>
                  <tr>
                    <th scope="col">ID</th>
                    <th scope="col">Nombre cliente</th>
                    <th scope="col">E-mail</th>
                    <th scope="col">País</th>
                    <th scope="col">Método de pago</th>
                    <th scope="col">Fecha de pago</th>
                    <th scope="col">Membresía</th>
                    <th scope="col">Expiración</th>
                  </tr>
                </thead>
                <tbody class="table-group-divider">
                  <tr>
                    <th scope="row"><%-data.id%></th>
                    <td><%-data.name_cliente%></td>
                    <td><%-data.email%></td>
                    <td><%-data.pais_cliente%></td>
                    <td><%-data.metodo_pago%></td>
                    <td><%-data.fecha_pago%></td>
                    <td><%-data.membresia%></td>
                    <td><%-data.exp_membresia%></td>
                  </tr>
                  
                </tbody>
              </table>


        <% }) %>
    </div>


    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.min.js"
    integrity="sha384-0pUGZvbkm6XF6gxjEnlmuGrJXVbNuzT9qBBavbLwCsOGabYfZo0T0to5eqruptLy"
    crossorigin="anonymous"></script>
</body>
</html>